<?xml version="1.0" encoding="utf-8"?>
<openerp>
  <data>
    <record id="view_company_family_commission_form" model="ir.ui.view">
        <field name="name">view.company.family.commission.form</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="account_commission.view_company_commission_form"/>
        <field name="arch" type="xml">		
        	<xpath expr="//field[@name='commission_priority4']" position="after">
        		<field name="commission_priority5"/>
        	</xpath>
        	<xpath expr="//field[@name='commission_product_id']" position="after">
        		<field name="pricelist_for_recompute"/>
        	</xpath>        	
        </field>
    </record>
  </data>
</openerp>